CREATE TABLE exe6 (
    id NUMBER(10),
    fname VARCHAR2(15),
    lname VARCHAR2(15),
    email VARCHAR2(30),
    hiredate VARCHAR2(20),
    jobid VARCHAR2(10),
    salary NUMBER(10),
    mid NUMBER(5),
    deptid NUMBER(5)
);

