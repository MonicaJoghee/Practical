SELECT * FROM exe6;

SELECT fname || ' ' || lname
FROM exe6
WHERE salary > (SELECT salary FROM exe6 WHERE id = 763);

SELECT fname || ' ' || lname AS name, salary, deptid, jobid
FROM exe6
WHERE jobid = (SELECT jobid FROM exe6 WHERE id = 769);

SELECT id, fname || ' ' || lname
FROM exe6
WHERE salary > (SELECT AVG(salary) FROM exe6);

SELECT *
FROM exe6
WHERE salary = (SELECT salary FROM exe6 WHERE fname = 'deepika');
